        </Label>

        <Label  x:Name="lblNombres"
                FontSize="18">
        </Label>

        <Label  x:Name="lblApellidos"
                FontSize="18">
        </Label>

        <Label  x:Name="lblFecha"
                FontSize="18">
        </Label>

        <Label  x:Name="lblCorreo"
                FontSize="18">
        </Label>

        <Label  x:Name="lblSalario"
                FontSize="18">
        </Label>

        <Label  x:Name="lblAporteIess"
                FontSize="18">
